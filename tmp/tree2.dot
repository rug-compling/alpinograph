    n0 -- n1;
    n0 -- t11;
    n1 -- n1t2;
    n1t2 -- t2 [dir="forward"];
    n1 -- n1n3 -- n3;
    n3 -- n3t4 -- t4;
    n3 -- n3t5 -- t5;
    n3 -- n3n6 -- n6;
    n6 -- n6t2 -- t2;
    n6 -- n6t5 -- t5;
    n6 -- n6t9 -- t9;
    n6 -- n6t10 -- t10;
